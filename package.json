  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@octokit/rest": "^18.3.5",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "14.14.34",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "@vercel/ncc": "0.27.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "prettier": "2.2.1",
    "typescript": "4.2.2"
